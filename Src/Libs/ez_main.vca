PROCEDURE Activate


	*--- LOAD STUDENT IMAGE
	LOCAL lcFile

	*--- ATTEMPT TO LOAD HI-RESOLUTION IMAGE		
	lcFile = ALLTRIM(goPreferences.txtHiResP.value) + ALLTRIM(N_STUDENTS.STUDENTID) + ".BMP"
	IF FILE( lcFile )
		THISFORM.imgLarge.Picture = lcFile
		RETURN
	ENDIF

	lcFile = ALLTRIM(goPreferences.txtHiResS.value) + ALLTRIM(N_STUDENTS.STUDENTID) + ".BMP"
	IF FILE( lcFile )
		THISFORM.imgLarge.Picture = lcFile
		RETURN
	ENDIF
	
	*--- ATTEMPT TO LOAD LOW-RESOLUTION IMAGE
	lcFile = ALLTRIM(goPreferences.txtLoResP.value) + ALLTRIM(N_STUDENTS.STUDENTID) + ".BMP"
	IF FILE( lcFile )
		THISFORM.imgLarge.Picture = lcFile
		RETURN
	ENDIF
		
	lcFile = ALLTRIM(goPreferences.txtLoResS.value) + ALLTRIM(N_STUDENTS.STUDENTID) + ".BMP"
	IF FILE( lcFile )
		THISFORM.imgLarge.Picture = lcFile
		RETURN
	ENDIF

	*--- ATTEMPT TO LOAD NO-PHOTO IMAGE
	lcFile = "GRAPHICS\NOPHOTO.BMP"
	IF FILE( lcFile )
		THISFORM.imgLarge.Picture = lcFile
	ENDIF

