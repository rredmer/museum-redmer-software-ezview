[START METHODS]
PROCEDURE cmdImport.Click

	*---- IMPORT RECORDS
	LOCAL lcPath, lcName, lcWorkArea, lnAns

	*---- VERIFY THAT THE USE DID NOT SELECT THE CURRENT DATA DIRECTORY!
	IF UPPER(ALLTRIM(THISFORM.txtDataPath.VALUE)) == UPPER(ALLTRIM(goRegistry.cDataPath))
		=MESSAGEBOX( "EZ-VIEW is currently using the source folder data.", MB_OK+MB_ICONEXCLAMATION, "ERROR" )
		RETURN
	ENDIF

	*---- VERIFY THE SOURCE DIRECTORY HAS A REGISTRY FILE	
	lcPath = THISFORM.txtDataPath.VALUE
	lcName = lcPath + "registry.dbf"
	IF ( NOT FILE( lcName ) )
		=MESSAGEBOX( "Source folder does not contain valid EZ-VIEW data.", MB_OK+MB_ICONEXCLAMATION, "ERROR" )
		RETURN
	ENDIF

	*---- VERIFY FROM USER
	lnAns = MESSAGEBOX( "Are you sure?", MB_YESNO+MB_ICONQUESTION, "IMPORT STUDENT DATA" )
	IF ( lnAns == IDNO )
		RETURN
	ENDIF

	*---- PROCESS RECORD DELETION
	IF THISFORM.cntErase.chkErase.Value = 1
		DELETE ALL IN N_STUDENTS
		DELETE ALL IN N_INCIDENT
		DELETE ALL IN N_AWARDS
		DELETE ALL IN N_ATYPES
		DELETE ALL IN N_SCHEDULE
		DELETE ALL IN N_CLASSES
	ENDIF

	
	*---- USE THE IMPORT TABLES
	lcWorkArea = ALIAS()
	goSQL.DB_TABLEOPEN(lcPath + "STUDENTS.DBF", "I_STUDENTS", "STUDENTID",  "EXCLUSIVE" )
	goSQL.DB_TABLEOPEN(lcPath + "INCIDENTS.DBF","I_INCIDENT", "STUDENTID",  "EXCLUSIVE" )
	goSQL.DB_TABLEOPEN(lcPath + "AWARDS.DBF",   "I_AWARDS",   "STUDENTID",  "EXCLUSIVE" )
	goSQL.DB_TABLEOPEN(lcPath + "AWARDTYP.DBF", "I_ATYPES",   "AWARDTYPE",  "EXCLUSIVE" )
	goSQL.DB_TABLEOPEN(lcPath + "SCHEDULE.DBF", "I_SCHEDULE", "STUDENTID",  "EXCLUSIVE" )
	goSQL.DB_TABLEOPEN(lcPath + "CLASSES.DBF",  "I_CLASSES",  "CLASSID",    "EXCLUSIVE" )
	
	*---- COPY DATA INTO APPLICATION TABLES
	SELECT I_STUDENTS
	SCAN
		SCATTER NAME loSTUDENT MEMO
		SELECT N_STUDENTS
		IF NOT SEEK(loSTUDENT.STUDENTID)
			APPEND BLANK
		ENDIF
		GATHER NAME loSTUDENT MEMO
		SELECT I_STUDENTS
	ENDSCAN

	SELECT I_INCIDENT
	SCAN
		SCATTER NAME loINCIDENT MEMO
		SELECT N_INCIDENT
		IF NOT SEEK(loINCIDENT.STUDENTID + STR(loINCIDENT.INCIDENTNO,4,0))
			APPEND BLANK
		ENDIF
		GATHER NAME loINCIDENT MEMO
		SELECT I_INCIDENT
	ENDSCAN

	SELECT I_AWARDS
	SCAN
		SCATTER NAME loAWARD MEMO
		SELECT N_AWARDS
		IF NOT SEEK(loAWARD.STUDENTID + STR(loAWARD.AWARDNO,4,0))
			APPEND BLANK
		ENDIF
		GATHER NAME loAWARD MEMO
		SELECT I_AWARDS
	ENDSCAN
	
	SELECT I_ATYPES
	SCAN
		SCATTER NAME loATYPE MEMO
		SELECT N_ATYPES
		IF NOT SEEK(loATYPE.AWARDTYPE)
			APPEND BLANK
		ENDIF
		GATHER NAME loATYPE MEMO
		SELECT I_ATYPES
	ENDSCAN

	SELECT I_SCHEDULE
	SCAN
		SCATTER NAME loSCHEDULE MEMO
		SELECT N_SCHEDULE
		IF NOT SEEK(loSCHEDULE.STUDENTID + STR(loSCHEDULE.CLASSNO,4,0))
			APPEND BLANK
		ENDIF
		GATHER NAME loSCHEDULE MEMO
		SELECT I_SCHEDULE
	ENDSCAN
		
	SELECT I_CLASSES
	SCAN
		SCATTER NAME loCLASSES MEMO
		SELECT N_CLASSES
		IF NOT SEEK(loCLASSES.CLASSID)
			APPEND BLANK
		ENDIF
		GATHER NAME loCLASSES MEMO
		SELECT I_CLASSES
	ENDSCAN
	
	*---- CLOSE IMPORT TABLES
	USE IN I_STUDENTS
	USE IN I_INCIDENT
	USE IN I_AWARDS
	USE IN I_ATYPES
	USE IN I_SCHEDULE
	USE IN I_CLASSES
	SELECT (lcWorkArea)
	
	=MESSAGEBOX( "IMPORT COMPLETE.", MB_OK+MB_ICONEXCLAMATION, "IMPORT" )

ENDPROC
[END METHODS]

