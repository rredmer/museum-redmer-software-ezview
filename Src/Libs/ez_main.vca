Name = "Command1"
[END PROPERTIES]
[START METHODS]
PROCEDURE Click

	*---- SELECT STUDENTS INTO PRINTFIL
	IF THISFORM.cntCriteria.chkAllRecords.Value
		
		SELECT PRINTFIL
		ZAP
		SELECT N_STUDENTS
		SCAN
			SCATTER NAME loSTUDENT MEMO
			SELECT PRINTFIL
			APPEND BLANK
			GATHER NAME loSTUDENT MEMO
			REPLACE PRINTFIL.QUERY_SEL WITH .T.
			SELECT N_STUDENTS
		ENDSCAN
		SELECT N_STUDENTS
		GOTO TOP IN PRINTFIL
		THISFORM.Refresh
		
	ELSE
		WITH THISFORM.cntCriteria
			
			lcWhere = .cboField1.Value + " " + .cboCompare1.value + "'" + ALLTRIM(.txtValue1.Value) + "'" + ;
					  IIF( .optExtend1.value < 3, IIF(.optExtend1.value=1," AND ", " OR ") + .cboField2.Value + " " + .cboCompare2.value + "'" + ALLTRIM(.txtValue2.Value) + "'", "" ) + ;
					  IIF( .optExtend2.value < 3, IIF(.optExtend2.value=1," AND ", " OR ") + .cboField3.Value + " " + .cboCompare3.value + "'" + ALLTRIM(.txtValue3.Value) + "'", "" )
			
			SELECT 0
			SELECT * ;
			FROM N_STUDENTS ;
			WHERE &lcWhere ;
			INTO CURSOR CUR_TMP
	
			IF _TALLY == 0
				=MESSAGEBOX( "No records match criteria.", MB_OK+MB_ICONEXCLAMATION, "WARNING!" )
			ENDIF
	
			SELECT PRINTFIL
			ZAP
			SELECT CUR_TMP
			SCAN
				SCATTER NAME loSTUDENT MEMO
				SELECT PRINTFIL
				APPEND BLANK
				GATHER NAME loSTUDENT MEMO
				REPLACE PRINTFIL.QUERY_SEL WITH .T.
				SELECT CUR_TMP
			ENDSCAN
			USE IN CUR_TMP
			SELECT N_STUDENTS
			GOTO TOP IN PRINTFIL
			THISFORM.Refresh

		ENDWITH	
	ENDIF
	

ENDPROC
PROCEDURE Error
LPARAMETERS nError, cMethod, nLine
	goSQL.Error(nError, cMethod, nLine)				&& Simply pass parms to db error handling method

ENDPROC
[END METHODS]

