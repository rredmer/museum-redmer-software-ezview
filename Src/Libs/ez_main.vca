PROCEDURE cmdDelete.Click


	*---- ERASE STUDENT
	LOCAL lnAns
	
	IF goSecurity.nSecureLevel < 3
		goSecurity.NoSecurity
		RETURN
	ENDIF
	
	lnAns = MESSAGEBOX("WARNING:  THIS WILL PERMANENTLY ERASE ALL INFORMATION PERTAINING TO " + CHR(13) + ;
					   ALLTRIM(CUR_LIST.FIRST_NAME) + " " + ALLTRIM(CUR_LIST.LAST_NAME) + " (STUDENT ID: " + ALLTRIM(CUR_LIST.studentid) + ")." + ;
					   CHR(13) + CHR(13) + ;
					   "ARE YOU SURE?", ;
					   MB_YESNO + MB_ICONQUESTION, "CONFIRM ERASE." )
	IF (lnAns == IDYES)
		
		SELECT N_STUDENTS
		SET ORDER TO TAG studentid
		IF SEEK( CUR_LIST.studentid )

			THISFORM.sbarMain.Panels(1).Text = "Erasing Student..."
			DELETE
			
			*---- DELETE AWARDS RECORDS
			DELETE FROM N_AWARDS   WHERE (N_AWARDS.STUDENTID == CUR_LIST.STUDENTID)
			
			*---- DELETE INCIDENTS RECORDS
			DELETE FROM N_INCIDENT WHERE (N_INCIDENT.STUDENTID == CUR_LIST.STUDENTID)
			
			*---- DELETE SCHEDULE RECORDS
			DELETE FROM N_SCHEDULE WHERE (N_SCHEDULE.STUDENTID == CUR_LIST.STUDENTID)
			THISFORM.sbarMain.Panels(1).Text = "Idle."
			
		ENDIF
		
		=MESSAGEBOX( "The record has been erased.", MB_OK+MB_ICONEXCLAMATION, "COMPLETE." )
		THISFORM.nSelect

	ENDIF

ENDPROC
