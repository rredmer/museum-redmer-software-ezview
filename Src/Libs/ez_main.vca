PROCEDURE cmdLocate.Click

	*--- CHANGE REPORT FILE PATH
	PRIVATE temp
	
	WITH THISFORM.txtReportForm
		temp = .value
		temp = GETFILE("FRX","Report", "Select", 1, "Locate Report Form")
		IF NOT EMPTY( temp )
			*--- IF NEW REPORT SELECTED
			IF ("Untitled" $ temp)
			
				SELECT N_STUDENTS
				CREATE REPORT ? IN SCREEN
				SELECT N_ATYPES
				.value = ""
			ELSE
				.value = temp
			ENDIF
			THISFORM.Refresh
		ENDIF
	ENDWITH
	
ENDPROC
