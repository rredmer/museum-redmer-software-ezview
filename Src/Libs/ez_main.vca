PROCEDURE cmdPrint.Click

	*---- PRINT STUDENT INFORMATION SHEET
	LOCAL lnAns
	IF goSecurity.nSecureLevel > 3
		lnAns = MESSAGEBOX( "Print report for current student?", MB_YESNO+MB_ICONQUESTION, "PRINT" )
		IF ( lnAns == IDYES )
			THISFORM.PrepPrint( "STUDENTID == '" + CUR_LIST.STUDENTID + "'" )
			SELECT PRINTFIL
			
			IF FILE(goRegistry.cReport)
				REPORT FORM (goRegistry.cReport) PREVIEW IN SCREEN
			ELSE
				=MESSAGEBOX( "Please set your default report using Prefences under the edit menu.", MB_OK+MB_ICONEXCLAMATION, "NO REPORT" )
			ENDIF
			
			SELECT N_STUDENTS
		ENDIF
	ELSE
		goSecurity.NoSecurity
	ENDIF

ENDPROC
