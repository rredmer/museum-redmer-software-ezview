PROCEDURE nselect


	*--- SELECT STUDENTS FOR LIST
	LOCAL lcWhere
	PUBLIC laCOMPARE[ 6 ]
	laCOMPARE[ 1 ] = "LIKE"
	laCOMPARE[ 2 ] = "="
	laCOMPARE[ 3 ] = "<>"
	laCOMPARE[ 4 ] = "<"
	laCOMPARE[ 5 ] = ">"
	laCOMPARE[ 6 ] = " $ "
	
	
	IF USED("CUR_LIST")
		USE IN CUR_LIST
	ENDIF

	SELECT 0
	IF THISFORM.cntMainList.chkAllRecords.Value
		SELECT LEFT(last_name,15) AS Last_Name,first_name,studentid ;
			FROM N_STUDENTS ;
			ORDER BY LAST_NAME ;
			INTO CURSOR CUR_LIST
	ELSE

		WITH THISFORM.cntMainList
			lcCompare = laCOMPARE[ .cboCompare.ListIndex ]
			IF .cboCompare.ListIndex < 6
				lcWhere = ALLTRIM( .cboField.value ) + " " + lcCompare + " '" + .txtValue.value + "'"
			ELSE
				lcWhere = "'" + .txtValue.value + "' $ " + ALLTRIM( .cboField.value )
			ENDIF
		ENDWITH

		WAIT "Retrieving student records.  Please wait." WINDOW NOWAIT
		SELECT LEFT(last_name,15) AS Last_Name,first_name,studentid ;
			FROM N_STUDENTS ;
			ORDER BY LAST_NAME ;
			WHERE &lcWhere. ;
			INTO CURSOR CUR_LIST
		WAIT CLEAR
	
	ENDIF

	IF _TALLY > 0	
		THISFORM.cntMainList.lstMain.RowSource = "CUR_LIST"
		SELECT N_STUDENTS
		SET ORDER TO TAG studentid
		SEEK CUR_LIST.studentid
		*** THISFORM.cntMain.dteBirthDate.date = IIF(DTOC( N_STUDENTS.BIRTHDATE ) == "  /  /  ", ;
		***									 DTOC( DATE() ), DTOC( N_STUDENTS.BIRTHDATE ))
	ENDIF
	
	WITH THISFORM
		.cntMainList.lblNumStudents.Caption = ALLTRIM(STR(RECCOUNT( "CUR_LIST" ))) + " Students"
		.sbarMain.Panels(2).Text = "User: " + ALLTRIM( goSecurity.cUser_ID )
		.sbarMain.Panels(3).Text = "Station: " + ALLTRIM( goRegistry.cStation_ID )
		.nLoadImage
		.cntMainList.lstMain.ListIndex = 1
		.Refresh
	ENDWITH
ENDPROC
PROCEDURE nloadimage


	*--- LOAD STUDENT IMAGE
	LOCAL lcFile
	

	*--- ATTEMPT TO LOAD LOW-RESOLUTION IMAGE
	lcFile = ALLTRIM(goPreferences.txtLoResP.value) + ALLTRIM(N_STUDENTS.studentid) + ".BMP"
	IF FILE( lcFile )
		THISFORM.cntMain.imgMain.Picture = lcFile
		RETURN
	ENDIF
		
	lcFile = ALLTRIM(goPreferences.txtLoResS.value) + ALLTRIM(N_STUDENTS.studentid) + ".BMP"
	IF FILE( lcFile )
		THISFORM.cntMain.imgMain.Picture = lcFile
		RETURN
	ENDIF

	*--- ATTEMPT TO LOAD HI-RESOLUTION IMAGE		
	lcFile = ALLTRIM(goPreferences.txtHiResP.value) + ALLTRIM(N_STUDENTS.studentid) + ".BMP"
	IF FILE( lcFile )
		THISFORM.cntMain.imgMain.Picture = lcFile
		RETURN
	ENDIF

	lcFile = ALLTRIM(goPreferences.txtHiResS.value) + ALLTRIM(N_STUDENTS.studentid) + ".BMP"
	IF FILE( lcFile )
		THISFORM.cntMain.imgMain.Picture = lcFile
		RETURN
	ENDIF
	
	*--- ATTEMPT TO LOAD NO-PHOTO IMAGE
	lcFile = "NOPHOTO.BMP"
	IF FILE( lcFile )
		THISFORM.cntMain.imgMain.Picture = lcFile
	ENDIF

ENDPROC
PROCEDURE prepprint

	LPARAMETERS lpcCriteria
	LOCAL lnNumRecs, lcFile
	
	SELECT PRINTFIL
	ZAP
	
	THISFORM.sBarMain.Panels(1).Text = "Retrieving records."
	
	APPEND FROM (goRegistry.cDataPath + "\STUDENTS.DBF") FOR &lpcCriteria.
	
	
	*--- LOAD EACH IMAGE INTO PHOTOGRAPH FIELD
	SELECT PRINTFIL
	lnNumRecs = RECCOUNT()
	SET ORDER TO
	GOTO TOP
	SCAN

		THISFORM.sBarMain.Panels(1).Text = "Loading image [" + ALLTRIM(STR(RECNO())) + "] of [" + ALLTRIM(STR(lnNumRecs)) + "]."
	
		*--- ATTEMPT TO LOAD HI-RESOLUTION IMAGE		
		lcFile = ALLTRIM(goPreferences.txtHiResP.value) + ALLTRIM(N_STUDENTS.studentid) + ".BMP"
		IF FILE( lcFile )
			APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
			LOOP
		ENDIF

		lcFile = ALLTRIM(goPreferences.txtHiResS.value) + ALLTRIM(N_STUDENTS.studentid) + ".BMP"
		IF FILE( lcFile )
			APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
			LOOP
		ENDIF

		*--- ATTEMPT TO LOAD LOW-RESOLUTION IMAGE
		lcFile = ALLTRIM(goPreferences.txtLoResP.value) + ALLTRIM(N_STUDENTS.studentid) + ".BMP"
		IF FILE( lcFile )
			APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
			LOOP
		ENDIF
		
		lcFile = ALLTRIM(goPreferences.txtLoResS.value) + ALLTRIM(N_STUDENTS.studentid) + ".BMP"
		IF FILE( lcFile )
			APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
			LOOP
		ENDIF

		*--- ATTEMPT TO LOAD NO-PHOTO IMAGE
		lcFile = "NOPHOTO.BMP"
		IF FILE( lcFile )
			APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
		ENDIF


		THISFORM.sBarMain.Panels(1).Text = "Idle."

	ENDSCAN
ENDPROC
