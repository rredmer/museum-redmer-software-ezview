PROCEDURE cmdCancel.Click

	*--- Exit the registry form
	IF EMPTY( THISFORM.txtStation_id.Value )
		=MESSAGEBOX( "Please enter a name for this work station.", MB_OK+MB_ICONSTOP, "Missing station name." )
		RETURN .F.
	ENDIF

	*--- Validate the data path
	IF ( NOT FILE( ALLTRIM(THISFORM.txtDataPath.text) + "REGISTRY.DBF" ) )
		WAIT "Please enter a valid data path." WINDOW
		RETURN .F.
	ENDIF

	=WritePrivStr("Defaults", "Station_id", THISFORM.txtStation_id.text, CURDIR() + THISFORM.APP_INIFILE)
	=WritePrivStr("Defaults", "DataPath", THISFORM.txtDataPath.text, CURDIR() + THISFORM.APP_INIFILE)
	=WritePrivStr("Defaults", "ReportDefault", THISFORM.txtReport.text, CURDIR() + THISFORM.APP_INIFILE)
	THISFORM.cStation_id = ALLTRIM(THISFORM.txtStation_id.text)
	THISFORM.cDataPath   = ALLTRIM(THISFORM.txtDataPath.text)
	THISFORM.cReport = ALLTRIM(THISFORM.txtReport.text)
	
	IF NOT USED("REGISTRY")
		USE (ALLTRIM(THISFORM.txtDataPath.text) + "REGISTRY.DBF" ) IN 0 SHARED
		THISFORM.PutValue( KEY_PRIMARY_HIRES_IMAGE_LOCATION, LEFT(THISFORM.txtDataPath.value,2) + "\IMAGES\" )
		THISFORM.PutValue( KEY_PRIMARY_LOWRES_IMAGE_LOCATION, LEFT(THISFORM.txtDataPath.value,2) + "\IMAGES\" )
		THISFORM.PutValue( KEY_SECONDARY_HIRES_IMAGE_LOCATION, LEFT(THISFORM.txtDataPath.value,2) + "\IMAGES\" )
		THISFORM.PutValue( KEY_SECONDARY_LOWRES_IMAGE_LOCATION, LEFT(THISFORM.txtDataPath.value,2) + "\IMAGES\" )
		USE IN REGISTRY
	ENDIF
	THISFORM.Hide

ENDPROC
