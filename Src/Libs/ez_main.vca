PROCEDURE cmdSave.Click

	*--- ADD NEW STUDENT
	LOCAL lcStudentID
	
	WITH THISFORM
		lcStudentID = .txtStudentID.Value
		IF NOT EMPTY( lcStudentID ) AND NOT EMPTY( .txtFirstName.Value ) AND NOT EMPTY( .txtLastName.Value )
			SELECT N_STUDENTS
			SET ORDER TO TAG STUDENTID
			IF NOT SEEK( lcStudentID )
				APPEND BLANK
				
				REPLACE N_STUDENTS.STUDENTID  WITH lcStudentID,;
						N_STUDENTS.FIRST_NAME WITH .txtFirstName.Value,;
						N_STUDENTS.LAST_NAME  WITH .txtLastName.Value
						
				.Hide
			ELSE
				=MESSAGEBOX( "Please enter a new student id.", MB_OK+MB_ICONSTOP, "Student already exists" )
			ENDIF
		ELSE
			=MESSAGEBOX( "Please student id, first name, and last name.", MB_OK+MB_ICONSTOP, "Missing information" )
		ENDIF
	ENDWITH
