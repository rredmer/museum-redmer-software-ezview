LPARAMETERS nError, cMethod, nLine
	goSQL.Error(nError, cMethod, nLine)				&& Simply pass parms to db error handling method

ENDPROC
PROCEDURE cmdSelect.Click

	*--- RUN REPORT
	LOCAL lnAns
	lnAns = MESSAGEBOX( "Run report?", MB_YESNO+MB_ICONQUESTION, "RUN REPORT" )
	IF ( lnAns == IDYES )
		SELECT PRINTFIL
		lnNumRecs = RECCOUNT()
		IF (lnNumRecs < 1)
			=MESSAGEBOX( "Please select records first.", MB_OK+MB_ICONSTOP, "No records selected." )
		ENDIF
		IF (lnNumRecs > 1000)
			=MESSAGEBOX( "Please narrow your search criteria to select fewer records.", MB_OK+MB_ICONSTOP, "Too many records selected." )
		ENDIF
		
		SET ORDER TO
		GOTO TOP
		SCAN

			THISFORM.msBarReport.Panels(1).Text = "Loading image [" + ALLTRIM(STR(RECNO())) + "] of [" + ALLTRIM(STR(lnNumRecs)) + "]."
	
			*--- ATTEMPT TO LOAD HI-RESOLUTION IMAGE		
			lcFile = ALLTRIM(goPreferences.txtHiResP.value) + ALLTRIM(printfil.studentid) + ".BMP"
			IF FILE( lcFile )
				APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
				LOOP
			ENDIF

			lcFile = ALLTRIM(goPreferences.txtHiResS.value) + ALLTRIM(printfil.studentid) + ".BMP"
			IF FILE( lcFile )
				APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
				LOOP
			ENDIF

			*--- ATTEMPT TO LOAD LOW-RESOLUTION IMAGE
			lcFile = ALLTRIM(goPreferences.txtLoResP.value) + ALLTRIM(printfil.studentid) + ".BMP"
			IF FILE( lcFile )
				APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
				LOOP
			ENDIF
		
			lcFile = ALLTRIM(goPreferences.txtLoResS.value) + ALLTRIM(printfil.studentid) + ".BMP"
			IF FILE( lcFile )
				APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
				LOOP
			ENDIF

			*--- ATTEMPT TO LOAD NO-PHOTO IMAGE
			lcFile = "NOPHOTO.BMP"
			IF FILE( lcFile )
				APPEND GENERAL PRINTFIL.PHOTOGRAPH FROM (lcFile)
			ENDIF

		ENDSCAN
		
		THISFORM.msBarReport.Panels(1).Text = "Running report."
		REPORT FORM (THISFORM.cReport) FOR (PRINTFIL.QUERY_SEL = .T.) PREVIEW IN SCREEN
		THISFORM.msBarReport.Panels(1).Text = "Idle."
		
	ENDIF
ENDPROC
[END METHODS]

